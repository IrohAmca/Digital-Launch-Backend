<h1>Posts</h1>
    <ul>
        {{#each posts}}
        <li class="card" onclick="postLansman('{{this.Genel_Bilgiler.Lansman_Adi}}')">
            <h2>{{this.Genel_Bilgiler.Lansman_Adi}}</h2>
            <p>Firma Adı: {{this.Genel_Bilgiler.Firma_Adi}}</p>
            <p>Başlangıç Tarihi: {{this.Genel_Bilgiler.Lansman_Başlangıc_Tarihi}}</p>
            <p>Bitiş Tarihi: {{this.Genel_Bilgiler.Lansman_Bitis_Tarihi}}</p>
            <p>Açıklama: {{this.Ust_Bolum.Açıklama_Yazısı}}</p>
        </li>
        {{/each}}
    </ul>

    <script>
        function postLansman(lansmanAdi) {
            fetch('/dl-onizle', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/x-www-form-urlencoded'
                },
                body: new URLSearchParams({
                    'name': lansmanAdi
                })
            })
            .then(response => response.text())
            .then(data => {
                console.log(data);
            })
            .catch(error => {
                console.error('Error:', error);
            });
        }
    </script>